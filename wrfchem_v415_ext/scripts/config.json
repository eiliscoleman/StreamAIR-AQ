             "Shannon": {"lon": -8.914729, "lat": 52.699406, "airport": true},
             "Dublin": {"lon": -6.26, "lat": 53.35, "airport": false},
             "Catania": {"lon": 15.065998, "lat": 37.468496, "airport": true},
             "Galway": {"lon": -9.06, "lat": 53.27, "airport": false},
             "Cork": {"lon": -8.4863, "lat": 51.8969, "airport": false},
             "London": {"lon": -0.1278, "lat": 51.5074, "airport": false},
             "Paris": {"lon": 2.3522, "lat": 48.8566, "airport": false},
             "Carnsore":{"lon": 2.3522, "lat": 52.1706, "airport": false},
             "Malinhead":{"lon": 7.3321, "lat": 55.3528, "airport": false}},
  "input-wps": "../../ECMWF-op",
  "output-wrf-raw": "../data/output-wrf-raw",
  "output-wrf": "../data/output-wrf",
  "output-mh": "../data/output-mh",
  "output-gw": "../data/output-gw",
  "output-ck": "../data/output-ck",
  "output-db": "../data/output-db", 
  "output-ln": "../data/output-ln",
  "output-pr": "../data/output-pr",
  "output-cp": "../data/output-cp",
  "output-mal": "../data/output-mal",
  "imgs": "../imgs",
  "mozbc-dir": "../../mozbc",
  "fmt": {
    "date": "%Y-%m-%d_%H:%M:%S"
  },
  "namelist": {
    "wps": "../WPS/namelist.wps",
    "wrf": "../WRFV3/run/namelist.input"
 },
  "execute": [
    "clean-wrf",
    "clean-op",
    "clean-linkgrib",
    "run-linkgrib",
    "run-update-namelists",
    "run-ungrib",
    "run-metgrid",
    "run-mv-wps",
    "run-ln-wrf",
    "run-update-namelist-nochem",
    "run-real",
    "run-mozbc-cams",
    "run-emission-preproc",
    "run-enh-emis",
    "run-update-namelist-chem",
    "run-wrf",
    "run-mv-wrf",
    "run-extract-dfiles",
    "run-make-png-json"
    ],
  "clean-linkgrib": {
    "program": "rm ./aq:* ./metgrid.log*",
    "dir": "../WPS",
    "check": false
  },
  "clean-op": {
    "program": "rm data/input-wrf/* data/output-wrf-raw/* data/output-wrf/*",
    "dir": "..",
    "check": false
  },
  "clean-wrf": {
    "program": "rm met_em* rsl.* wrfout* wrfb* wrfi* wrfrst* wrffire* wrfchemi*",
    "dir": "../WRFV3/run",
    "check": false
  },
  "run-mozbc-cam": {
    "program": "./mozbc < CRIMECH-601-newwmacc.inp > moz_cam.out",
    "dir": "../mozbc",
    "check": true
  },
  "run-mozbc-cams": {
    "program": "./mozbc < CRIMECH-601-cams.inp > moz_cams.out",
    "dir": "../mozbc",
    "check": true
  },
  "run-linkgrib": {
    "program": "./link_grib.csh ../../ECMWF-op/*",
    "dir": "../WPS",
    "check": true
  },
  "run-extract-dfiles": {
    "program": "./components/extract-dfiles.py",
    "dir": ".",
    "check": true
  },
  "run-extract-csv": {
    "program": "./components/extract-from-dfiles-op.py",
    "dir": ".",
    "check": true
  },
  "run-ln-wrf": {
    "program": "ln -sf ../../data/input-wrf/met_em* .",
    "dir": "../WRFV3/run",
    "check": true
  },
  "run-metgrid": {
    "program": "./metgrid.exe",
    "dir": "../WPS",
    "check": true
  },
  "run-mv-wps": {
    "program": "mv met_em* ../data/input-wrf",
    "dir": "../WPS",
    "check": true
  },
  "run-mv-wrf": {
    "program": "mv $(ls wrfout* | tail -96) ../../data/output-wrf-raw",
    "dir": "../WRFV3/run",
    "check": true
  },
  "run-make-png-json":{
   "program": "./components/streamair_postproc_new.sh",
    "dir": ".",
    "check": false
  },     
 "run-real": {
    "program": "mpiexec -n 48 ./real.exe",
    "dir": "../WRFV3/run",
    "check": true
  },
  "run-ungrib": {
    "program": "./ungrib.exe",
    "dir": "../WPS",
    "check": true
  },
  "run-update-namelists": {
    "program": "./components/update-namelists.py",
    "dir": ".",
    "check": true
  },
  "run-update-namelist-chem": {
    "program": "./components/update_namelist_chem.py",
    "dir": ".",
    "check": true
  },
  "run-update-namelist-nochem": {
    "program": "./components/update_namelist_nochem.py",
    "dir": ".",
    "check": true
  },
  "run-emission-preproc": {
    "program": "./components/create_emis_crimech_leapyear.py",
    "dir": ".",
    "check": true
  },
  "run-enh-emis": {
    "program": "./components/other_cities_solid_fuel_em_enh.py",
    "dir": ".",
    "check": true
  },
  "run-wrf": {
    "program": "mpiexec -n 72 ./wrf.exe",
    "dir": "../WRFV3/run",
    "check": true
  }
}
